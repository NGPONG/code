<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--这种方式也可以添加SQL的连接字符串-->
  <connectionStrings>
    <add name="SqlStr2" connectionString="server=(local);uid=sa;pwd=kissyou199;database=damon"/>
    <add name="SqlStr3" connectionString="asd"/>
  </connectionStrings>
  
  <!--添加一个Sql的连接字符串的信息-->
  <appSettings>
    <add  key="SqlStr" value="server=(local);uid=sa;pwd=kissyou199;database=damon"/>
  </appSettings>

  <!--添加一个应用程序配置信息-->
  <appSettings>
    <!--
     key：设置当前子节点的key
     value：设置当前子节点key所对应的value
     -->
    <add  key="myName" value="我是吴鹏哈哈哈"/>
  </appSettings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>